{
	"targets": {

		"xenial-amd64/gcc": {
			"buildenv": "xenial-amd64",
			"builddeps": [
				"autoconf",
				"autoconf-archive",
				"automake",
				"binutils",
				"ccache",
				"clang",
				"clang-format",
				"doxygen",
				"g++",
				"gawk",
				"gcc",
				"git",
				"git-svn",
				"gzip",
				"help2man",
				"libc++-dev",
				"libc++abi-dev",
				"libflac-dev",
				"libltdl-dev",
				"libmpg123-dev",
				"libogg-dev",
				"libpulse-dev",
				"libsdl1.2-dev",
				"libsdl2-dev",
				"libsndfile1-dev",
				"libtool",
				"libtool-bin",
				"libvorbis-dev",
				"libxml-xpath-perl",
				"make",
				"mscompress",
				"p7zip",
				"p7zip-full",
				"pkg-config",
				"portaudio19-dev",
				"subversion",
				"subversion-tools",
				"tar",
				"tcc",
				"unzip",
				"zip",
				"zlib1g-dev",
				"build-essential"
			],
			"environment": {
				"CCACHE_CPP2": "1"
			},
			"buildcmd": [
				"sudo bash -c \" echo 'deb http://archive.ubuntu.com/ubuntu/ xenial-updates main restricted universe multiverse' >> /etc/apt/sources.list \" ",
				"sudo add-apt-repository ppa:ubuntu-toolchain-r/test",
				"sudo apt update",
				"sudo apt full-upgrade -y",
				"sudo apt install gcc-9 g++-9 libstdc++-9-dev binutils",
				"PATH=/usr/lib/ccache:$PATH make -j${PARALLEL} CC=gcc-9 CXX=g++-9 LD=g++-9 STRICT=1",
				"PATH=/usr/lib/ccache:$PATH make -j${PARALLEL} CC=gcc-9 CXX=g++-9 LD=g++-9 STRICT=1 check"
			]
		}

	}
}
